{
	"name": "wp-multi-block-starter",
	"version": "0.1.0",
	"description": "Example block scaffolded with Create Block tool.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "rm -rf build && npm run build:compiled && npm run build:interactive && npm run build-blocks-manifest",
		"format": "wp-scripts format && eslint --fix './src/**/*.{js,jsx}'",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "eslint './src/**/*.{js,jsx}' --fix",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"build:compiled": "wp-scripts build --webpack-copy-php --config webpack.compiled.js",
		"start:compiled": "wp-scripts start --webpack-copy-php",
		"build:interactive": "wp-scripts build --experimental-modules",
		"start:interactive": "wp-scripts start --experimental-modules",
		"build-blocks-manifest": "wp-scripts build-blocks-manifest",
		"postbuild": "npm run build-blocks-manifest",
		"start:all": "concurrently \"npm run start\" \"npm run start:interactive\""
	},
	"devDependencies": {
		"@commitlint/cli": "^19.6.1",
		"@commitlint/config-conventional": "^19.6.0",
		"@wordpress/blocks": "^13.8.0",
		"@wordpress/dom-ready": "^4.8.0",
		"@wordpress/scripts": "^30.0.2",
		"husky": "^9.1.7",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.1.4",
		"concurrently": "^8.0.0"
	},
	"dependencies": {
		"@wordpress/env": "^10.8.0"
	}
}
